{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Attach to GameEditor",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "name": "Launch Front-end (Chrome)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}/GameEditor.Client"
    }
  ]
}
